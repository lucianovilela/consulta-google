<!DOCTYPE html>
<html lang="pt-br">
  <%- include header %>
  <body>
    <%- include nav %>
    <div class="section no-pad-bot" id="index-banner">
      <div class="container">
        <div class="row">
          <form class="col s12" action="signo">
            <div class="row">
              <div class="input-field col s6">
                <input placeholder="Celebridade" id="first_name" type="text" name="nome"/>
                <label for="first_name">Celebridade</label>
                <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                   <i class="material-icons right">send</i>
                </button>
            </div>
          </div>
          </form>
        </div>
      </div >
          <% if(result){ %>
      <div class="card center">
          <div class="card-title">

            <img class="responsive-img" src="<%= result.imagem %>" height="20%" width="20%"/>
          </div>
          <div class="card-content">
            <p><%= result.nome %></p>
            <p><strong>Data Nascimento </strong><%= dn %></p>
            <p><strong>Signo </strong><%= result.signo %></p>
          </div>
          </div>
          <% } %>
        </div>
      </div>
    </div>
    <%- include footer %>
  </body>
</html>
